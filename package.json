{
    "name": "app-server",
    "version": "0.1.0",
    "private": true,
    "main": "src/index.js",
    "scripts": {
        "start": "pm2 start ecosystem.config.json --no-daemon",
        "dev": "cross-env NODE_ENV=development nodemon src/index.js",
        "predev2": "set DEBUG=dev",
        "dev2": "DEBUG=dev nodemon src/index.js",
        "commit": "node ./src/utils/commit.js",
        "prettier": "prettier --write **/*.js",
        "prettier_check": "prettier --check **/*.js",
        "lint": "eslint .",
        "lint_fix": "eslint . --fix"
    },
    "keywords": [
        "node",
        "node.js",
        "express",
        "rest",
        "api",
        "mongodb",
        "mongoose",
        "es6",
        "es7",
        "es8",
        "es9",
        "passport",
        "joi",
        "eslint",
        "prettier"
    ],
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cloudinary": "^1.27.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "cross-env": "^7.0.0",
        "debug": "^2.6.9",
        "dotenv": "^10.0.0",
        "express": "^4.16.1",
        "express-mongo-sanitize": "^2.0.0",
        "express-rate-limit": "^5.0.0",
        "helmet": "^4.1.0",
        "http-errors": "^1.6.3",
        "http-status": "^1.4.0",
        "joi": "^17.6.0",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "mongoose": "^6.0.6",
        "morgan": "^1.9.1",
        "multer": "^1.4.3",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.0",
        "pm2": "^5.2.0",
        "swagger-jsdoc": "^6.0.8",
        "swagger-ui-express": "^4.1.6",
        "validator": "^13.6.0",
        "winston": "^3.2.1",
        "xss-clean": "^0.1.1"
